.dial-all	
	position relative
	display flex
	justify-content center
	align-items center
.dial-percentages
	& circle 
		position absolute
		fill rgba(0,0,0,0)
		stroke white
		stroke-width 5
		stroke-dasharray 282px 282px

	& circle:nth-child(2n) 
		position absolute
		fill rgba(0,0,0,0)
		stroke redColor
		stroke-width 5

	&_text
		position absolute
		color #9d9d9d
		font-size 40px
		font-family 'LatoRegular'

